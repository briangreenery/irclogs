		"fzaninotto/faker"  : "dev-master",
		"jasonlewis/basset" : "dev-master",
		"julien-c/mongovel" : "dev-master#f32fa9788df522b8ef89da8ba4c3abc0583b9255"
	},
	"autoload": {
